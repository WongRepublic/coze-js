{
  "name": "@coze/api",
  "version": "0.1.0",
  "description": "Coze Javascript library",
  "homepage": "https://github.com/coze-dev/coze-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/coze-dev/coze-js.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/node/index.js",
  "browser": "dist/web/index.js",
  "types": "dist/node/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist/node && tsc -b tsconfig.json",
    "build:web": "rm -rf dist/web && tsc -b tsconfig.web.json && mv dist/web/shims/web/* dist/web/shims",
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint . --cache",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "start": "rm -rf dist/node && tsc -b -w tsconfig.json",
    "start:web": "rm -rf dist/web && tsc -b -w tsconfig.web.json && mv dist/web/shims/web/* dist/web/shims",
    "test": "jest --coverage --config=jest.config.cjs ./test/*"
  },
  "dependencies": {
    "undici": "^6.19.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/eslint-plugin": "^7.25.1",
    "@swc/core": "^1.3.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^20",
    "@types/whatwg-fetch": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "jest": "^29.3.0",
    "lint-staged": "^12.3.2",
    "prettier": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.1",
    "ts-jest": "^29.0.3",
    "typescript": "~5.0.4"
  }
}
